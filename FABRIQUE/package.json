{
  "name": "fabrique",
  "version": "1.0.0",
  "description": "Fabrication API",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.js --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --config jest.config.js --detectOpenHandles",
    "test:cov": "jest --coverage --config jest.config.js --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js --detectOpenHandles",
    "test:e2e": "jest --config jest-e2e.config.js --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "cache-manager": "^5.2.0",
    "helmet": "^6.0.0",
    "compression": "^1.7.4",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "16.11.12",
    "@types/supertest": "^2.0.16",
    "jest": "28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
