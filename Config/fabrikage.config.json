{
  "system": {
    "name": "Fabrikage Production System",
    "version": "1.0.0",
    "environment": "production",
    "logLevel": "info"
  },
  "paths": {
    "root": "D:\\busineshuboffline CHATGTP\\Master",
    "core": "D:\\busineshuboffline CHATGTP\\Master\\Core",
    "logs": "D:\\busineshuboffline CHATGTP\\Master\\Logs",
    "formulas": "D:\\busineshuboffline CHATGTP\\Master\\Formulas",
    "routines": "D:\\busineshuboffline CHATGTP\\Master\\Routines"
  },
  "modules": {
    "autoExecution": {
      "enabled": true,
      "schedule": "0 * * * *"
    },
    "coreProtocols": {
      "enabled": true,
      "apiEndpoint": "http://localhost:3000/api"
    },
    "intelligenceMatrix": {
      "enabled": true,
      "learningRate": 0.01,
      "maxIterations": 1000
    },
    "observabilityAtlas": {
      "enabled": true,
      "retentionDays": 30,
      "metricsEnabled": true
    },
    "provenanceLedger": {
      "enabled": true,
      "blockchainEnabled": false
    },
    "promptProcessing": {
      "enabled": true,
      "maxTokens": 4096,
      "temperature": 0.7
    }
  },
  "security": {
    "authenticationRequired": true,
    "jwtSecret": "your-secret-key-here",
    "passwordSaltRounds": 10
  },
  "api": {
    "port": 3000,
    "corsEnabled": true,
    "rateLimit": {
      "windowMs": 15 * 60 * 1000,
      "max": 100
    }
  },
  "logging": {
    "level": "info",
    "file": "fabrikage.log",
    "maxSize": "10m",
    "maxFiles": 5
  }
}
